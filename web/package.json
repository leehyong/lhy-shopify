{
  "name": "shopify-app-template-node",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "debug": "node --inspect-brk index.js",
    "dev": "tsc && cross-env NODE_ENV=development nodemon index.js --ignore ./frontend",
    "serve": "tsc && cross-env NODE_ENV=production node index.js",
    "test": "vitest --reporter=verbose",
    "product_test": "tsc && cross-env TS_NODE_PROJECT='product/__tests__/tsconfig.test.json' mocha product/__tests__/**.js"
  },
  "mocha": {
    "require": [
      "ts-node/register",
      "tsconfig-paths/register"
    ],
    "ui": "bdd"
  },
  "type": "module",
  "engines": {
    "node": ">=16.13.0"
  },
  "dependencies": {
    "@shopify/shopify-api": "^3.1.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cross-env": "^7.0.3",
    "exceljs": "^4.3.0",
    "express": "^4.17.3",
    "serve-static": "^1.14.1"
  },
  "devDependencies": {
    "jsonwebtoken": "^8.5.1",
    "nodemon": "^2.0.15",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "supertest": "^6.2.2",
    "vitest": "^0.9.2"
  }
}
